"select new com.projecte.dtos.EventFindAll(e1_0.event_id,e1_0.event_name, new com.projecte.dtos.ClubDTO(c1_0.club_id, c1_0.club_name), new com.projecte.dtos.BranchDTO(e2_1.branch_id,e2_1.year,e2_1.branch_name) from event e1_0 left join club c1_0 on c1_0.club_id=e1_0.club_id left join ( event_eligibility e2_0 join branch e2_1 on e2_1.branch_id=e2_0.branch_id) on e1_0.event_id=e2_0.event_id"

